{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": ["es2019","dom"],
    "declaration": true,
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": ".",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": ["./src/**/*", "./test/**/*"],
  "exclude": ["./node_modules", "./dist"]
}
